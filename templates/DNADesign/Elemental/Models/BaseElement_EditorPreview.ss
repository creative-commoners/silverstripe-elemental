<div class="elementalpreview">
    <a href="$CMSEditLink" class="elementaledit">
        <div class="elementalpreview__icon">$Icon</div>

        <div class="elementalpreview__detail">
            <h2><% if Title %>$Title <% end_if %><small>$Type</small></h2>

            <% if Summary %>
                <p>$Summary</p>
            <% end_if %>
        </div>
    </a>
</div>
